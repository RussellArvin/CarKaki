# Database connection string
# Format: postgresql://USER:PASSWORD@HOST:PORT/DATABASE
DATABASE_URL="postgresql://postgres:password@localhost:5432/your_database"

# Development mode flag
# Set to "true" for development environment
IS_DEVELOPMENT="true"

# Google Maps API Key for server-side operations
# Get from Google Cloud Console
GOOGLE_MAPS_API_KEY="your_google_maps_api_key_here"

# Google Maps API Key for client-side operations
# Same as above or separate key with restricted domains
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY="your_public_google_maps_api_key_here"

# URA API Access Key
# Get from URA API Services
URA_ACCESS_KEY="your_ura_access_key_here"

# Clerk Webhook Secret
# Get from Clerk Dashboard > Webhooks
CLERK_WEBHOOK_SECRET="your_clerk_webhook_secret_here"

# Node environment
# Options: "development", "production", "test"
NODE_ENV="development"